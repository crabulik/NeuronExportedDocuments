@using NeuronExportedDocuments.Resources
@model NeuronExportedDocuments.Models.ViewModels.BlockedViewModel

@{
    ViewBag.Title = MainMessages.rs_GetNeuronDocumentBlockedCaption;
    Layout = "~/Views/_Layout.cshtml";
}

@section Scripts {
   
}
@section Body {
    <h2>@MainMessages.rs_GetNeuronDocumentBlockedCaption</h2>
    <div class="alert alert-danger" role="alert">
        <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
        <span class="sr-only">@MainMessages.rs_Error</span>
        @Html.Raw(string.Format(MainMessages.rs_BlockedDocumentInfo, Model.ServiceDocumentInfo.PublishId, Model.SupportContact))
    </div>
    @Html.ActionLink(MainMessages.rs_GetNextDocument, "Index")
}